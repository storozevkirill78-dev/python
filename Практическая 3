#Задание 5
f = int(input())
if f % 10 ==1 and f %100 !=11:
    word = "программист"
elif 2<= f % 10 <= 4 and (f % 100 <10 or f % 100>= 20):
    word ="программиста"
else:
    word ="программистов"
print(f"{f} {word}")

#Задание 6
a = str(input("Входные данные: \n >>> "))
b = int(a)
c = b // 1000
d = b - c * 1000
e = c // 100
f = c - e * 100
g = f // 10
h = f - g * 10
i = d // 100
j = d - i * 100
k = j // 10
l = j - k * 10
sum1 = e + g + h
sum2 = i + k + l
if sum1 == sum2:
    print("Выходные данные: \n >>> Счастливый")
else:
    print("Выходные данные: \n >>> Не счастливый")
